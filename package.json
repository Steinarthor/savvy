{
    "name": "my-react-app",
    "version": "0.1.0",
    "scripts": {
        "build": "npm run clean && npm-run-all build:*",
        "build:webpack": "webpack -p",
        "prod": "npm run clean && NODE_ENV=production npm run build:webpack -- --env=prod",
        "clean": "npm-run-all --parallel clean:*",
        "clean:bsb": "bsb -clean-world",
        "clean:project": "rm -rf bundledOutputs .merlin",
        "start": "npm-run-all --parallel start:*",
        "start:webpack": "webpack-dev-server",
        "test": "npm run jest",
        "test:watch": "npm run clean && npm run jest -- --watchAll",
        "jest": "jest"
    },
    "keywords": [
        "BuckleScript"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "bs-loader": "^2.0.7",
        "bs-platform": "^5.2.1",
        "css-loader": "^3.4.0",
        "file-loader": "^5.0.2",
        "node-sass": "^7.0.0",
        "npm-run-all": "^4.1.5",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.1.2",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "dependencies": {
        "@glennsl/bs-json": "^5.0.1",
        "bs-css": "^11.0.0",
        "bs-fetch": "^0.5.0",
        "re-classnames": "^4.1.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "reason-react": "^0.7.0"
    }
}
